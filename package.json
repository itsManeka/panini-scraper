{
    "name": "panini-scraper",
    "version": "1.2.0",
    "description": "A TypeScript library for scraping Panini Brasil product information",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*",
        "README.md",
        "LICENSE",
        "CHANGELOG.md",
        "ARCHITECTURE.md"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --maxWorkers=1",
        "test:ci": "jest --coverage --ci --watchAll=false --maxWorkers=1",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "prepare": "npm run build",
        "example:basic": "ts-node examples/basic-usage.ts",
        "example:advanced": "ts-node examples/advanced-usage.ts",
        "example:batch": "ts-node examples/batch-usage.ts",
        "example:html": "ts-node examples/test-with-real-html.ts"
    },
    "keywords": [
        "panini",
        "scraper",
        "batch-scraping",
        "library",
        "typescript",
        "clean-architecture",
        "web-scraping",
        "comics",
        "books",
        "nodejs"
    ],
    "author": "Emanuel Ozorio Dias",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/itsmaneka/panini-scraper.git"
    },
    "bugs": {
        "url": "https://github.com/itsmaneka/panini-scraper/issues"
    },
    "homepage": "https://github.com/itsmaneka/panini-scraper#readme",
    "dependencies": {
        "axios": "^1.5.0",
        "cheerio": "^1.0.0-rc.12"
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "@types/node": "^20.6.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.49.0",
        "jest": "^29.7.0",
        "rimraf": "^5.0.1",
        "ts-jest": "^29.1.1",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
